{
    "name": "starry-night-theme",
    "version": "0.0.1",
    "displayName": "Starry Night Theme",
    "homepage": "https://github.com/buffDev/starry-night-theme",
    "description": "Dark theme created by Buff!",
    "publisher": "Danil0ws",
    "license": "MIT",
    "scripts": {
        "build": "node ./scripts/build.js",
        "lint": "node ./scripts/lint.js",
        "package": "vsce package -o ./bin/starry-night-theme.vsix",
        "vscode:prepublish": "npm run build",
        "vsce-publish": "vsce publish"
    },
    "maintainers": [
        "Danil0ws <mydanilows@gmail.com>"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/buffDev/starry-night-theme.git"
    },
    "bugs": {
        "url": "https://github.com/buffDev/starry-night-theme/issues"
    },
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Themes"
    ],
    "keywords": [
        "starry-night",
        "buffdev",
        "theme",
        "color-theme"
    ],
    "icon": "icon.png",
    "galleryBanner": {
        "color": "#001F3F",
        "theme": "dark"
    },
    "contributes": {
        "themes": [
            {
                "label": "Starry Night Theme",
                "uiTheme": "vs-dark",
                "path": "./theme/starry-night-theme.json"
            }
        ]
    },
    "devDependencies": {
        "js-yaml": "^3.13.1",
        "prettier": "^1.19.1",
        "tinycolor2": "^1.4.1",
        "vsce": "^2.15.0"
    },
    "theme": {
        "colors": {
            "activityBar.background": "#001F3F",
            "activityBar.foreground": "#FFFFFF",
            "activityBarBadge.background": "#0074D9",
            "activityBarBadge.foreground": "#FFFFFF",
            "badge.background": "#0074D9",
            "badge.foreground": "#FFFFFF",
            "button.background": "#0074D9",
            "button.foreground": "#FFFFFF",
            "button.hoverBackground": "#0063B1",
            "debugToolBar.background": "#001F3F",
            "diffEditor.insertedTextBackground": "#2ECC40",
            "diffEditor.removedTextBackground": "#FF4136",
            "dropdown.background": "#001F3F",
            "dropdown.border": "#0074D9",
            "editor.background": "#001F3F",
            "editor.findMatchBackground": "#FFDC00",
            "editor.findMatchHighlightBackground": "#FFDC00",
            "editor.foreground": "#FFFFFF",
            "editor.lineHighlightBackground": "#001F3F",
            "editor.selectionBackground": "#0074D9",
            "editor.selectionHighlightBackground": "#0074D9",
            "editor.selectionHighlightBorder": "#0074D9",
            "editor.wordHighlightBackground": "#0074D9",
            "editor.wordHighlightStrongBackground": "#0074D9",
            "editorCursor.foreground": "#FFFFFF",
            "editorGroup.border": "#0074D9",
            "editorGroupHeader.tabsBackground": "#001F3F",
            "editorIndentGuide.activeBackground": "#FFFFFF",
            "editorIndentGuide.background": "#FFFFFF",
            "editorLineNumber.activeForeground": "#FFFFFF",
            "editorLineNumber.foreground": "#FFFFFF",
            "editorWhitespace.foreground": "#FFFFFF",
            "editorWidget.background": "#001F3F",
            "editorWidget.border": "#0074D9",
            "editorSuggestWidget.background": "#001F3F",
            "editorSuggestWidget.border": "#0074D9",
            "editorSuggestWidget.foreground": "#FFFFFF",
            "editorSuggestWidget.highlightForeground": "#FFFFFF",
            "editorSuggestWidget.selectedBackground": "#0074D9",
            "editorHoverWidget.background": "#001F3F",
            "editorHoverWidget.border": "#0074D9",
            "editorHoverWidget.foreground": "#FFFFFF"
        }
    }
}
